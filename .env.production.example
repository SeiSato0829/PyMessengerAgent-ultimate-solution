# Render Production Environment Variables
# これらの値はRenderダッシュボードで設定してください

# データベース設定（Renderが自動設定）
DATABASE_URL=postgres://user:password@hostname:port/database

# Supabase互換の設定（RenderのPostgreSQLを使用）
NEXT_PUBLIC_SUPABASE_URL=YOUR_RENDER_APP_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=dummy_anon_key_for_render
SUPABASE_SERVICE_KEY=dummy_service_key_for_render

# アプリケーション設定
NEXT_PUBLIC_APP_URL=https://your-render-app.onrender.com
NODE_ENV=production

# 暗号化キー（Renderで生成）
ENCRYPTION_KEY=your-32-character-encryption-key

# その他
PORT=3000